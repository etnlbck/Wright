    "map":"map.png",
    "sprites":"sprites.png",
    "tiles":"tiles.png",
    "title":"title.png",
    "defaultsprites":"../default/sprites.png",
    "scanlines":"../default/scanlines.png"
  },
  "stencils":{
    "codeDeathCheck":{ "when":{ "_":[ "variable", "health", "isLessEqualThan", 0 ] }, "as":{ "_":"scene" }, "set":{ "state":"death" } },
    "codeItemAdd":[
      {
        "when":{ "_":[ { "_":[ "variable", "inventory" ] }, "search", { "itemId":{ "_":[ "variable", "addItem" ] } } ] },
        "execute":{ "sum":1, "to":{ "_":[ "that", "quantity" ] } },
        "elseExecute":[
          { "as":{ "_":[ "new", { "_":[ "stencil", "dataItems", { "_":[ "variable", "addItem" ] } ] } ] }, "pushInto":{ "_":[ "variable", "inventory" ] } },
          { "when":{ "_":[ "variable", "firstLogLine" ] }, "as":{ "_":"variable" }, "set":{ "logLine":{ "_":[ "variable", "firstLogLine" ] } } }
        ]
      },
      { "_":[ "stencil", "codeLogLine" ] }
    ],
    "codeItemConsume":{
      "when":{ "_":[ { "_":[ "variable", "inventory" ] }, "search", { "itemId":{ "_":[ "variable", "consumeItem" ] } } ] },
      "execute":[
        { "subtract":1, "to":{ "_":[ "that", "quantity" ] } },
        {
          "when":{ "_":[ "that", "quantity", "isEqualTo", 0 ] },
          "execute":[
            { "unpush":{ "_":"that" }, "from":{ "_":[ "variable", "inventory" ] } },
            {
              "when":{ "_":[ "variable", "currentItem", "isEqualTo", { "_":"that" } ] },
              "as":{ "_":"variable" },
              "set":{ "currentItem":{ "_":[ "variable", "inventory", 0 ] }, "selectedItem":0 }
            }
          ]
        }
      ]
    },
    "codeLogLine":[
      {
        "as":{ "_":"variable" },
        "execute":{
          "when":{ "_":[ "variable", "lastLog" ] },
          "execute":{
            "object":{
              "set":{ "_":[ "stencil", "logLine" ] },
              "y":{ "_":{ "_":[ "variable", "lastLog", "y", "+", 16 ] } },
              "html":{ "_":[ "variable", "logLine" ] },
              "moveUp":{ "_":[ "variable", "lastLog", "moveUp" ] }
            },
            "into":{ "_":"scenehud" },
            "execute":{ "set":{ "lastLog":{ "_":"that" } } }
          },
          "elseExecute":{
            "object":{ "set":{ "_":[ "stencil", "logLine" ] }, "html":{ "_":[ "variable", "logLine" ] } },
            "into":{ "_":"scenehud" },
            "execute":{ "set":{ "lastLog":{ "_":"that" } } }
          }
        }
      },
      { "as":{ "_":[ "object", "logLine" ] }, "execute":{ "sum":16, "to":{ "_":[ "this", "moveUp" ] } } }
    ],
    "codeNothing":[ { "as":{ "_":"variable" }, "set":{ "logLine":"Nothing happened." } }, { "_":[ "stencil", "codeLogLine" ] } ],
    "codeScreenUpdate":[
      { "as":{ "_":[ "objectTyped", "currentItemSprite" ] }, "set":{ "_":[ "variable", "currentItem" ] } },
      { "as":{ "_":[ "objectTyped", "currentItemLabel" ] }, "set":{ "html":{ "_":[ "variable", "currentItem", "label" ] } } },
      { "as":{ "_":[ "objectTyped", "currentItemQuantity" ] }, "set":{ "html":{ "_":[ "x", ".", { "_":[ "variable", "currentItem", "quantity" ] } ] } } },
      {
        "as":{ "_":"variable" },
        "set":{ "areaCounter":0, "direction":{ "_":[ "stencil", "dataDirections", { "_":[ "objectTyped", "playersign", "angle" ] } ] } }
      },
      {
        "times":12,
        "execute":[
          {
            "as":{ "_":"variable" },
            "set":{
              "curArea":{
                "_":[
                  "objectTyped",
                  "playersign",
                  "complexCollidesWith",
                  { "subject":{ "_":[ "object", "area" ] }, "hitbox":{ "_":[ "variable", "direction", "view", { "_":[ "variable", "areaCounter" ] } ] } }
                ]
              },
              "curItem":{
                "_":[
                  "objectTyped",
                  "playersign",
                  "complexCollidesWith",
                  { "subject":{ "_":[ "object", "item" ] }, "hitbox":{ "_":[ "variable", "direction", "view", { "_":[ "variable", "areaCounter" ] } ] } }
                ]
              },
              "ground":{ "_":[ "objectTyped", { "_":[ "ground", ".", { "_":[ "variable", "areaCounter" ] } ] } ] },
              "item":{ "_":[ "objectTyped", { "_":[ "item", ".", { "_":[ "variable", "areaCounter" ] } ] } ] },
              "row":{ "_":[ "variable", "areaCounter", "/", 3, "floor" ] },
              "wall":{ "_":[ "objectTyped", { "_":[ "wall", ".", { "_":[ "variable", "areaCounter" ] } ] } ] }
            }
          },
          {
            "when":{ "_":[ "variable", "curArea", "visibility" ] },
            "execute":[
              { "as":{ "_":[ "variable", "curArea" ] }, "set":{ "visible":1 } },
              { "as":{ "_":[ "variable", "curItem" ] }, "set":{ "visible":{ "_":[ "variable", "curItem", "visibility" ] } } }
            ]
          },
          {
            "when":{ "_":[ "variable", "areaCounter", "isLessThan", 3 ] },
            "execute":{
              "when":{ "_":[ "variable", "curArea", "wallStencil" ] },
              "as":{ "_":[ "variable", "wall" ] },
              "set":{ "frame":0, "visible":1, "bgcolor":"" },
              "elseExecute":{ "as":{ "_":[ "variable", "wall" ] }, "set":{ "visible":0 } }
            },
            "elseExecute":{
              "when":{ "_":[ "variable", "curArea", "wallStencil" ] },
              "execute":[
                {
                  "as":{ "_":[ "variable", "wall" ] },
                  "set":[
                    { "visible":1, "bgcolor":{ "_":[ "variable", "curArea", "wallStencil", "bgcolors", { "_":[ "variable", "row" ] } ] } },
                    { "_":[ "variable", "curArea", "wallStencil" ] }
                  ]
                },
                { "as":[ { "_":[ "variable", "ground" ] }, { "_":[ "variable", "item" ] } ], "set":{ "visible":0, "state":"default" } }
              ],
              "elseExecute":{
                "when":{ "_":[ "variable", "curArea", "groundStencil" ] },
                "execute":[
                  {
                    "as":{ "_":[ "variable", "ground" ] },
                    "set":[
                      { "visible":1, "bgcolor":{ "_":[ "variable", "curArea", "groundStencil", "bgcolors", { "_":[ "variable", "row" ] } ] } },
                      { "_":[ "variable", "curArea", "groundStencil" ] }
                    ]
                  },
                  { "as":{ "_":[ "variable", "wall" ] }, "set":{ "visible":0 } },
                  {
                    "when":{ "_":[ "variable", "curItem" ] },
                    "as":{ "_":[ "variable", "item" ] },
                    "set":[ { "visible":{ "_":[ "variable", "curItem", "visibility" ] } }, { "_":[ "variable", "curItem", "itemStencil" ] } ],
                    "elseExecute":{ "as":{ "_":[ "variable", "item" ] }, "set":{ "visible":0, "state":"default" } }
                  }
                ],
                "elseExecute":{ "as":[ { "_":[ "variable", "ground" ] }, { "_":[ "variable", "object" ] }, { "_":[ "variable", "wall" ] } ], "set":{ "visible":0 } }
              }
            }
          },
          { "sum":1, "to":{ "_":[ "variable", "areaCounter" ] } }
        ]
      }
    ],
    "codeTurn":[
      {
        "when":{ "_":[ "variable", "inventory", "search", { "itemId":"shield" } ] },
        "as":{ "_":"that" },
        "execute":{ "as":{ "_":"variable" }, "set":{ "damage":{ "_":[ "that", "quantity" ] } } },
        "elseExecute":{ "as":{ "_":"variable" }, "set":{ "damage":0 } }
      },
      {
        "as":{ "_":[ "object", "foe" ] },
        "execute":{
          "when":{ "_":[ "this", "complexCollidesWith", { "subject":{ "_":[ "objectTyped", "playersign" ] }, "hitbox":{ "width":24, "height":24, "x":-8, "y":-8 } } ] },
          "subtract":{ "_":[ "this", "damage" ] },
          "to":{ "_":[ "variable", "damage" ] }
        }
      },
      {
        "when":{ "_":[ "variable", "damage", "isLessThan", 0 ] },
        "execute":[
          { "when":{ "_":[ "cheat", "unlimitedHealth", "not" ] }, "sum":{ "_":[ "variable", "damage" ] }, "to":{ "_":[ "variable", "health" ] } },
          { "as":{ "_":"variable" }, "set":{ "logLine":{ "_":[ "You're under attack! ", ".", { "_":[ "variable", "damage" ] }, ".", "HP" ] } } },
          { "_":[ "stencil", "codeLogLine" ] },
          { "_":[ "stencil", "codeDeathCheck" ] }
        ]
      }
    ],
