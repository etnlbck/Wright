            "execute":[
              { "subtract":{ "_":[ "variable", "currentItem", "quantity" ] }, "to":{ "_":[ "this", "health" ] } },
              {
                "when":{ "_":[ "this", "health", "isLessEqualThan", 0 ] },
                "execute":[
                  { "as":{ "_":"variable" }, "set":{ "happened":1, "logLine":{ "_":[ "that", "label", ".", " died!" ] } } },
                  { "_":[ "stencil", "codeLogLine" ] },
                  {
                    "when":{ "_":[ "this", "drop" ] },
                    "object":{ "set":{ "_":[ "stencil", { "_":[ "that", "drop" ] } ] }, "x":{ "_":[ "that", "x" ] }, "y":{ "_":[ "that", "y" ] } },
                    "elseExecute":{
                      "when":{ "_":[ "variable", "health", "isLessThan", 4 ] },
                      "object":{ "set":{ "_":[ "stencil", "potion" ] }, "x":{ "_":[ "that", "x" ] }, "y":{ "_":[ "that", "y" ] } },
                      "elseExecute":{ "object":{ "set":{ "_":[ "stencil", "gold" ] }, "x":{ "_":[ "that", "x" ] }, "y":{ "_":[ "that", "y" ] } } }
                    }
                  },
                  { "remove":1 }
                ],
                "elseExecute":[
                  {
                    "as":{ "_":"variable" },
                    "set":{
                      "happened":1,
                      "logLine":{ "_":[ "You hit ", ".", { "_":[ "that", "label" ] }, ".", ". ", ".", { "_":[ "variable", "currentItem", "quantity" ] }, ".", "DMG" ] }
                    }
