    "modelPartLogo":{
      "states":{
        "code":[
          {
            "name":"Physics",
            "data":{
              "walls":{ "area":{ "width":256, "height":120, "x":0, "y":-30 }, "execute":{ "set":{ "forceY":{ "_":[ "randomNumber", [ -10, -5 ] ] } } } },
              "gravityY":1
            }
          },
          {
            "name":"Sequence",
            "data":[ { "wait":4 }, { "set":{ "bgcolor":{ "_":[ "randomValue", [ "#f00", "#0f0", "#00f", "#ff0", "#0ff", "#fff" ] ] } } }, { "loopTo":0 } ]
          }
        ]
      }
    },
    "modelStage":{
