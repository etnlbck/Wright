                  "data":{ "_":[ "that", "blockData" ] },
                  "set":{ "_":[ "that", "blockData", "icon" ] },
                  "image":"sprites", "width":16, "height":16,
                  "zIndex":110, "x":{ "_":[ "that", "counter", "%", 6, "*", 16, "+", 160 ] }, "y":{ "_":[ "that", "counter", "/", 6, "floor", "*", 16, "+", 20 ] },
                  "execute":{
