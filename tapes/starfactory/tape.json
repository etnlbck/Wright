                        { "object":{ "set":{ "_":[ "stencil", "modelPopupLabel" ] }, "html":{ "_":[ "", ".", "-", ".", { "_":[ "that", "data", "price" ] } ] } } },
                        { "as":{ "_":[ "this", "complexCollidesWith", { "subject":{ "_":[ "object", "placedblocks" ] }, "all":1 } ] }, "execute":{ "remove":1 } },
                        { "_":[ "stencil", "cloud" ] },
                        {},
                        { "object":{ "set":[ { "_":[ "stencil", "modelBlock" ] }, { "_":[ "that", "data" ] } ] } },
                        { "_":[ "stencil", "codeUpdateUi" ] }
                      ],
                      "elseExecute":{ "object":{ "set":{ "_":[ "stencil", "modelPopupLabel" ] }, "color":"#f00", "html":"NEED STARS!" } }
                    }
