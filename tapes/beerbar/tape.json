                    {
                      "when":{ "_":[ "keyUp", "isHit" ] },
                      "execute":[
                        { "when":{ "_":[ "this", "slot", "isEqualTo", 0 ] }, "set":{ "slot":3 }, "elseExecute":{ "subtract":1, "to":{ "_":[ "this", "slot" ] } } },
                        { "executeAction":"setSlot" }
