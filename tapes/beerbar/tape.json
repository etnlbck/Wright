      "zIndex":10,
      "animations":{ "despair":{ "frame":4 }, "prepare":{ "frame":1 }, "run":{ "frames":[ 2, 3 ], "loopTo":0 }, "stand":{ "frame":0 }, "win":{ "frame":5 } },
